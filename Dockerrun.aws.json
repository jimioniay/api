{
  "AWSEBDockerrunVersion": "2",
  "containerDefinitions": [
    {
      "name": "api",
      "image": "jimipayne/api",
      "memory": 256,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 443,
          "hostPort": 443,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "DEV_SERVER_PORT",
          "value": "80"
        }
      ]
    }
  ]
}
